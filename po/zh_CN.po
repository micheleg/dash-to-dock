#: prefs.js:34
msgid "Main Settings"
msgstr "主设置"

#: prefs.js:43
msgid "Dock is fixed and always visible"
msgstr "Dock 固定且总是可见"

#: prefs.js:59
msgid "Animation time [ms]"
msgstr "动画时间(毫秒)"

#: prefs.js:70
msgid "Show delay [ms]"
msgstr "显示延迟(毫秒)"

#: prefs.js:81
msgid "Hide delay [ms]"
msgstr "隐藏延迟(毫秒)"

#: prefs.js:96
msgid "Autohide"
msgstr "自动隐藏"

#: prefs.js:103
msgid "intellihide"
msgstr "智能隐藏"

#: prefs.js:131
msgid "Application based intellihide"
msgstr "应用程序基于智能隐藏"

#: prefs.js:146
msgid "Show the dock on following monitor (if attached)"
msgstr "Dock 将在如下显示器显示(如果已连接)"

#: prefs.js:148
msgid "Primary (default)"
msgstr "主显示器(默认)"

#: prefs.js:149
msgid "1"
msgstr "1"

#: prefs.js:150
msgid "2"
msgstr "2"

#: prefs.js:151
msgid "3"
msgstr "3"

#: prefs.js:152
msgid "4"
msgstr "4"

#: prefs.js:177
msgid "Max height"
msgstr "最大高度"

#: prefs.js:195
msgid "Expand (experimental and buggy)"
msgstr "全部展开(实验阶段且存在问题)"

#: prefs.js:224
msgid "Maximum icon size"
msgstr "最大图标尺寸"

#: prefs.js:229
msgid "16"
msgstr "16"

#: prefs.js:230
msgid "24"
msgstr "24"

#: prefs.js:231
msgid "32"
msgstr "32"

#: prefs.js:232
msgid "48"
msgstr "48"

#: prefs.js:233
msgid "64"
msgstr "64"

#: prefs.js:255
msgid "Show favorite application icons"
msgstr "显示收藏的程序图标"

#: prefs.js:260
msgid "Show running application icons"
msgstr "显示正在运行的程序图标"

#: prefs.js:276
msgid "Optional features"
msgstr "可选功能"

#: prefs.js:282
msgid "Customize the dock background opacity"
msgstr "自定义 Dock 背景透明度"

#: prefs.js:299
msgid "Opacity"
msgstr "透明度"

#: prefs.js:315
msgid "Only when in autohide"
msgstr "仅当自动隐藏时"

#: prefs.js:334
msgid "Switch workspace when scrolling over the dock"
msgstr "当滚过 Dock 时切换工作区"

#: prefs.js:345
msgid "Switch one workspace at a time"
msgstr "一次切换一个工作区"

#: prefs.js:351
msgid "Only a 1px wide area close to the screen edge is active"
msgstr "仅当靠近屏幕边缘1像素宽区域时启用"

#: prefs.js:356
msgid "All the area of the dock is active"
msgstr "Dock 全部区域时启用"

#: prefs.js:378
msgid "Customize actions on mouse click"
msgstr "自定义鼠标点击动作"

#: prefs.js:393
msgid "Action on clicking on running app"
msgstr "点击正在运行程序的动作"

#: prefs.js:395
msgid "Do nothing (default)"
msgstr "无动作(默认)"

#: prefs.js:396
msgid "Minimize"
msgstr "最小化"

#: prefs.js:397
msgid "Launch new window"
msgstr "启动新窗口"

#: prefs.js:398
msgid "Cycle through application windows"
msgstr "循环浏览应用程序窗口"

#: prefs.js:409
msgid "Minimize window on shift+click (double click for all app windows)"
msgstr "当按下“Shift+单击”(双击适用于全部程序窗口)时最小化窗口"
